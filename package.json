{
  "name": "vitesse-webext-mv3",
  "displayName": "Vitesse WebExt MV3",
  "version": "0.0.1",
  "description": "[description]",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "pnpm run clear && cross-env NODE_ENV=development TARGET_BROWSER=firefox run-p dev:*",
    "dev:prepare": "esno scripts/prepare.ts",
    "dev:web": "vite",
    "build": "cross-env NODE_ENV=production run-s clear build:web",
    "build:web": "vite build",
    "pack": "cross-env NODE_ENV=production run-p pack:*",
    "pack:zip": "rimraf extension.zip && jszip-cli add dist -o ./extension.zip",
    "pack:crx": "crx pack dist -o ./extension.crx",
    "pack:xpi": "cross-env WEB_EXT_ARTIFACTS_DIR=./ web-ext build --source-dir ./dist --filename extension.xpi --overwrite-dest",
    "release:firefox": "cross-env TARGET_BROWSER=firefox run-s build pack:xpi",
    "release:chromium": "cross-env TARGET_BROWSER=chrome run-s build pack:crx",
    "start:chromium": "web-ext run --source-dir ./dist --target=chromium",
    "start:firefox": "web-ext run --source-dir ./dist --target=firefox-desktop",
    "start:firefoxdev": "web-ext run --source-dir ./dist --target=firefox-desktop --firefox=firefoxdeveloperedition",
    "clear": "rimraf dist extension.*",
    "lint": "prettier --check . && eslint --cache 'src/**/*.{json,ts,js,vue}'",
    "fix": "eslint --cache 'src/**/*.{json,ts,js,vue}' --fix",
    "format": "prettier --write .",
    "check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "devtools:options": "vue-devtools",
    "devtools:popup": "cross-env PORT=8099 vue-devtools",
    "update-icon-fonts": "shx cp \"node_modules/primeicons/fonts/*\" src/assets/"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.16.1",
    "@ffflorian/jszip-cli": "^3.1.6",
    "@iconify-json/prime": "^1.1.1",
    "@types/chrome": "^0.0.180",
    "@types/download": "8.0.1",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^17.0.23",
    "@types/webextension-polyfill": "^0.8.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/compiler-sfc": "^3.2.31",
    "@vue/devtools": "^6.1.4",
    "@vueuse/core": "^7.7.1",
    "body-parser": "1.19.0",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "crx": "^5.0.1",
    "download": "8.0.0",
    "echarts": "^5.3.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "esno": "^0.14.1",
    "express": "4.17.1",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "kolorist": "^1.5.1",
    "launch-editor-middleware": "^2.3.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.1",
    "primeflex": "^3.1.3",
    "primeicons": "^5.0.0",
    "primevue": "^3.12.4",
    "rimraf": "^3.0.2",
    "safe-compare": "1.1.4",
    "shx": "^0.3.4",
    "typescript": "^4.6.3",
    "typescript-vue-plugin": "^0.30.6",
    "unplugin-auto-import": "^0.6.9",
    "unplugin-icons": "^0.14.1",
    "unplugin-vue-components": "^0.18.5",
    "vite": "^2.9.1",
    "vite-plugin-web-extension": "^1.1.3",
    "vue": "^3.2.31",
    "vue-demi": "^0.12.5",
    "vue-echarts": "^6.0.2",
    "vue-tsc": "^0.33.9",
    "web-ext": "^6.8.0",
    "webext-bridge": "^5.0.4",
    "webextension-polyfill": "^0.8.0"
  },
  "lint-staged": {
    "*.{ts,vue}": [
      "eslint --cache --fix",
      "prettier --ignore-unknown --write"
    ]
  }
}
